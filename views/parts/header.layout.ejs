<header>
    <nav class="navbar navbar-light bg-light">
        <a class="navbar-brand">LOGO</a>
        <div class="form-inline">
            <button class="btn btn-primary my-2 my-sm-0 mr-2" data-toggle="modal" data-target="#modalSignIn">Sign In</button>
            <button class="btn btn-primary my-2 my-sm-0"   data-toggle="modal" data-target="#modalSignUp">Sign Up</button>
        </div>
    </nav>
    <div class="modal fade" id="modalSignIn" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Sign In</h5>
                    <form>
                        <br>
                        <br>
                        <div class="form-group row">
                            <label for="inputEmail" class="col-sm-2 col-form-label">Email</label>
                            <div class="col-sm-10">
                                <input type="email" class="form-control" id="inputEmailSingIn" placeholder="Email">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inputPassword" class="col-sm-2 col-form-label">Pass</label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" id="inputPasswordSingIn" placeholder="Password">
                            </div>
                        </div>
                    </form>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" id="logBtn">Submit</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="modalSignUp" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Sign Up</h5>
                    <form>
                        <br>
                        <br>
                        <div class="form-group row">
                            <label for="inputEmail" class="col-sm-2 col-form-label">Email</label>
                            <div class="col-sm-10">
                                <input type="email" class="form-control" id="inputEmailSingUp" placeholder="Email">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inputPassword" class="col-sm-2 col-form-label">Pass</label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" id="inputPasswordSingUp" placeholder="Password">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="inputPassword" class="col-sm-2 col-form-label">Pass</label>
                            <div class="col-sm-10">
                                <input type="password" class="form-control" id="inputConfirmPasswordSingUp" placeholder="Password">
                            </div>
                        </div>
                    </form>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" id="regBtn">Submit</button>
                </div>
            </div>
        </div>
    </div>
</header>